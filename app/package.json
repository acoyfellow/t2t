{
  "name": "t2t-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "alchemy dev",
    "deploy": "alchemy deploy",
    "destroy": "alchemy destroy",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:better-auth": "bunx @better-auth/cli generate",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:local": "wrangler d1 migrations apply t2t-db --local",
    "db:remote": "wrangler d1 migrations apply t2t-db --remote",
    "postinstall": "node patches/fix-alchemy-d1.js"
  },
  "devDependencies": {
    "@lucide/svelte": "^0.562.0",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.3",
    "drizzle-kit": "^0.31.8",
    "svelte": "^5.46.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  },
  "dependencies": {
    "ai": "beta",
    "@ai-sdk/mcp": "beta",
    "@openrouter/ai-sdk-provider": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "alchemy": "^0.82.1",
    "better-auth": "1.4.7",
    "drizzle-orm": "^0.45.1",
    "effect": "^3.19.13"
  },
  "type": "module"
}
